    <ImageView
        android:id="@+id/scissors2"
        android:layout_width="101dp"
        android:layout_height="74dp"
        android:layout_marginBottom="136dp"
        android:layout_marginStart="148dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/paper"
        app:srcCompat="@drawable/scissors" />

    <ImageView
        android:id="@+id/pencil"
        android:layout_width="101dp"
        android:layout_height="72dp"
        android:layout_marginBottom="120dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="96dp"
        app:layout_constraintBottom_toTopOf="@+id/paper"
        app:layout_constraintEnd_toStartOf="@+id/scissors2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/rock"
        app:srcCompat="@drawable/pencil" />

    <ImageView
        android:id="@+id/paper"
        android:layout_width="101dp"
        android:layout_height="74dp"
        android:layout_marginBottom="136dp"
        android:layout_marginStart="104dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/pencil2"
        app:srcCompat="@drawable/paper" />

    <ImageView
        android:id="@+id/scissors"
        android:layout_width="101dp"
        android:layout_height="74dp"
        android:layout_marginBottom="8dp"
        android:layout_marginStart="96dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toTopOf="@+id/pencil"
        app:layout_constraintStart_toEndOf="@+id/rock"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.92"
