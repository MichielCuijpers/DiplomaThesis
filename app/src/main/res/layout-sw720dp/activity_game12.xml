        android:textColor="@android:color/black"
        android:textSize="40dp"
        app:layout_constraintBottom_toTopOf="@+id/scissors2"
        app:layout_constraintEnd_toStartOf="@+id/pencil3"
        app:layout_constraintHorizontal_bias="0.707"
        app:layout_constraintStart_toEndOf="@+id/paper" />

    <ImageView
        android:id="@+id/paper3"
        android:layout_width="101dp"
        android:layout_height="74dp"
        android:layout_marginBottom="136dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="72dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/help_button9"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/scissors2"
        app:srcCompat="@drawable/paper" />

    <ImageView
        android:id="@+id/pencil3"
        android:layout_width="101dp"
        android:layout_height="74dp"
        android:layout_marginBottom="20dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="72dp"
        app:layout_constraintBottom_toTopOf="@+id/paper3"
        app:layout_constraintEnd_toStartOf="@+id/help_button9"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/scissors2"
        app:srcCompat="@drawable/pencil" />

    <ImageView
        android:id="@+id/paper2"
        android:layout_width="101dp"
        android:layout_height="74dp"
        android:layout_marginBottom="8dp"
        android:layout_marginStart="156dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toTopOf="@+id/scissors2"
        app:layout_constraintStart_toEndOf="@+id/scissors"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.611"
        app:srcCompat="@drawable/paper" />

    <ImageView
        android:id="@+id/pencil2"
        android:layout_width="101dp"
        android:layout_height="74dp"
        android:layout_marginBottom="136dp"
